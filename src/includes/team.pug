mixin card(member)
    .card
        img.card__image(src='assets/img/team/'+member.img, alt=member.name)
        .card__right
            p.card__name= member.name
            .card__description
                .card__role
                    .bi.bi-lightbulb
                    p= member.role
                .card__contact
                    if member.github
                        a.button.button--icon.bi.bi-github(href=member.github)
                    if member.web_mail
                        a.button.button--icon.bi.bi-globe(href=member.web_mail)
                    if member.linkedin
                        a.button.button--icon.bi.bi-linkedin(href=member.linkedin)

section#team
    .team__container
        h2.font-bold Nuestro 
            span.text-wildWatermelon Equipo
            | 
        .team__list
            each member in team
                +card(member)
#workshop-team-slider.splide
    .splide__track 
        ul.splide__list 
            li.splide__slide 
                .container
                    each member in team.slice(0, 4)
                        +card(member)
            li.splide__slide 
                .container
                    each member in team.slice(4, 8)
                        +card(member)
            li.splide__slide 
                .container
                    each member in team.slice(8, 12)
                        +card(member)
            li.splide__slide 
                .container
                    each member in team.slice(12, 16)
                        +card(member)
            li.splide__slide
                .container
                    each member in team.slice(16, 20)
                        +card(member)
            li.splide__slide
                .container
                    each member in team.slice(20, 24)
                        +card(member)
            
                                  